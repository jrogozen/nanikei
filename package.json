  "description": "japanese verb conjugation quiz",
  "main": "server/index.js",
  "scripts": {
    "start": "npm run babel-node:prod -- dist/server.js",
    "seed": "NODE_ENV=development SEED=true node server",
    "clean": "rm -rf dist",
