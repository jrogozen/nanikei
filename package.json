    "babel-node:prod": "NODE_ENV=production babel-node --presets es2015,stage-0 --ignore node_modules,dist",
    "build:dev:server": "NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server",
    "build:dev:client": "NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-client",
    "dev": "NODE_ENV=development nodemon",
    "postinstall": "npm run clean && npm run build:dev:server && npm run build:dev:client"
  },
  "engines": {
    "node": "4.2.4",
    "npm": "2.14.x"
